{
  "manifest_version": 2,
  "name": "Slack Notify",
  "version": "0.0.1",

  "background":  {
    "scripts": ["background.js"],
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": { "default_icon": "slack.png" },
  "permissions": ["http://*/", "https://*/", "alarms", "storage", "tabs"]
}
