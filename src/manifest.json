{
  "manifest_version": 2,
  "name": "Slack Notifier (DEV)",
  "version": "999",
  "version_name": "unreleased",

  "background":  {
    "scripts": ["background.js"],
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": { "default_icon": "slack.png" },
  "permissions": ["http://*/", "https://*/", "alarms", "notifications", "storage", "tabs"]
}
